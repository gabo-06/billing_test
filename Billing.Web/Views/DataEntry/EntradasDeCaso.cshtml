            console.log('estoy estoy estoy estoy estoy estoy estoy estoy estoy estoy ');
            var respuesta;

            swal({               
                async: false,
                title: titulo,
                text: mensaje,
                type: "warning",
                showCancelButton: true,
                confirmButtonColor: "#DD6B55",
                confirmButtonText: "Yes",
                cancelButtonText: "No",
                closeOnConfirm: false,
                closeOnCancel: true
            },
            function (isConfirm)
            {       
                if (isConfirm)
                {
                    
                    respuesta = true;
                }
                else
                    respuesta = false;

                // 
                // miFuncion(isConfirm, objetoArgumentos);
                // miFuncion();
            })

            return respuesta;
        }

